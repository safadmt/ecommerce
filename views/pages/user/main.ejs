<style>
  .imgcategory {
    max-width: 483px;
    max-height: 230px;
    object-fit: contain;
  }
  #imagecollectiondiv {
    background-image: url('/productcollectionimage/jpeg-optimizer_casey-connell-wGeNkndPSn4-unsplash (1).jpeg') !important;
  background-repeat: no-repeat;
  background-size: cover;
    height: 300px !important;
  }
  #imagecollectiondiv2 {
    background-image: url('/productcollectionimage/jpeg-optimizer_pexels-the-5th-50003-179909 (2).jpeg') !important;
  background-repeat: no-repeat;
  background-size: cover;
    height: 300px !important;
  }
  
  
  
  .coupon {
    background-color: #973131;
    padding: 10px 0px 6px 0px;
    text-align: center;
    
  }
  .coupon p{
    color: white;
    font-size: 100% !important;
  }
  
  @media only screen and (min-width: 700px) {
  .image-product-collection{
    object-fit: contain;
    min-height: 432px !important;
    max-height: 432px !important;
  }
  .imgbanner{
    object-fit: cover !important;
    max-height: 700px !important;
  }
  .coupon p {
    font-size: 80%;
  }
}
</style>
<main id="mainpage">
  <!-- coupon -->
  
  <!-- slider Area Start -->
  <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
      <% banners.forEach((banner,index)=> {%> 
        <% if(index === 0) {%> 
      <div class="carousel-item active imgdiv">
        <img class="d-block w-100 imgbanner" src="/banners/<%= banner.imageurl%>" alt="First slide">
        <div class="carousel-caption ">
          <h1><%=banner.first_caption%></h1>
          <h4><%=banner.second_caption%></h4>
        </div>
      </div>
      
      <% } else { %>
        <div class="carousel-item">
          <img class="d-block w-100 imgbanner" src="/banners/<%= banner.imageurl%>" alt="First slide">
          <div class="carousel-caption ">
            <h1><%=banner.first_caption%></h1>
          <h4><%=banner.second_caption%></h4>
          </div>
        </div>
      <% } %>
    <% }) %>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
  <% if(wallet) {%>
    <div>
      <div class="coupon"><p><%= wallet.description %></p></div>
    </div>
    <% } %>
  <!-- slider Area End-->
  <!-- Category Area Start-->
  <section class="category-area section-padding30">
    <div class="container-fluid">
      <!-- Section Tittle -->
      <div class="row">
        <div class="col-lg-12">
          <div class="section-tittle text-center mb-85">
            <h2>Shop by Collection</h2>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xl-6 col-lg-6">
          <div class="single-category mb-30">
            <div
              class="category-img "
              id="imagecollectiondiv"
              >://
              
              <div class="category-caption">
                <h2 style="color: white !important">Men`s</h2>
                <span class=""
                  ><a href="/collection/Mens" class="genric-btn danger circle">Shop collection</a></span
                >
                <span class="collection"><br />New Collection</span>
              </div>
            </div>
          </div>
        </div>

        <div class="col-xl-6 col-lg-6">
          <div class="single-category mb-30">
            <div
              class="category-img "
              id="imagecollectiondiv2"
            >
              <div class="category-caption" id="">
                <h2 style="color: white !important">Women's</h2>
                <span 
                  ><a href="/collection/Womens" class="genric-btn danger circle">Shop collection</a></span
                >
                <span class="collection"><br />New Collection</span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- Category Area End-->
  <!-- Latest Products Start -->
  <section class="latest-product-area padding-bottom">
    <div class="container">
      <div class="row product-btn d-flex justify-content-end align-items-end">
        <!-- Section Tittle -->
        <div class="col-xl-4 col-lg-5 col-md-5">
          <div class="section-tittle mb-30">
            <h2>Latest Products</h2>
          </div>
        </div>
        <div class="col-xl-8 col-lg-7 col-md-7">
          <div class="properties__button f-right">
            <!--Nav Button  -->
            <nav>
              <div class="nav nav-tabs" id="nav-tab">
                <a
                  class="nav-item nav-link"
                  id="nav-home-tab"
                  href="/new-products"
                  >View All</a
                >
                
              </div>
            </nav>
            <!--End Nav Button  -->
          </div>
        </div>
      </div>
      <!-- Nav Card -->
      <div class="tab-content" id="nav-tabContent">
        <!-- card one -->
        <div
          class="tab-pane fade show active"
          id="nav-home"
          role="tabpanel"
          aria-labelledby="nav-home-tab"
        >
          <div class="row">
            <% products.forEach((item,index)=> {%>

            <div class="col-xl-4 col-lg-4 col-md-6">
              <div class="single-product mb-60">
                <div class="product-img">
                  <a href="/product/<%= item._id%>"
                    ><img src="/products/images/<%= item.images[0] %>" alt="" class="image-product-collection"
                  /></a>
                  <div class="new-product">
                    <span>New</span>
                  </div>
                </div>
                <div class="product-caption">
                  
                  <h4><a href="#"><%= item.brand%> | <%= item.gender%></a></h4>

                  <div class="price">
                    <ul>
                      <li>Rs.<%= item.price %></li>
                      <% if(item.discount_in_percentage) {%>
                      <li class="discount">
                        <%= item.price * item.discount_in_percentage / 100%>
                      </li>
                      <% } %>
                    </ul>
                  </div>
                  <% if(item.stock_available !== 0) {%>
                    <button
                      class="genric-btn primary-border circle"
                      onclick="addToCart(event,'<%= item._id%>')"
                    >
                      Add to Cart
                    </button>
                    <button
                      class="genric-btn primary-border circle mt-2"
                      onclick="addToWishlist(event,'<%= item._id%>')"
                    >
                      Add to Wishlist
                    </button>
                    <% } else {%>
                      <button class="genric-btn danger circle">Out of stock</button>
                    <% } %>
                </div>
              </div>
            </div>

            <% }) %>
            <!-- Card two -->
          </div>
        </div>
      </div>
      <!-- End Nav Card -->
    </div>
  </section>
  <!-- Latest Products End -->
  <!-- Best Product Start -->
  <div class="best-product-area lf-padding">
    <div
      class="product-wrapper bg-height"
      style="background-image: url('https://images.unsplash.com/photo-1649357585015-179ed98f513d?q=80&w=1452&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D')"
    >
      <div class="container position-relative">
        <div class="row justify-content-between align-items-end">
          
          <div class="col-xl-8 col-lg-8">
            <div class="best-product-caption">
              <h2 style="color: white !important;">
                Find The Best Product<br />
                from Our Shop
              </h2>
              <p style="color: white !important;">Indulge in the finest watches that define and<br>
                 sophistication 
                 elegance for every occasion. <br> 
                
              <a href="/shop" class="black-btn">Shop Now</a>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Shape -->
    
  </div>
  <div class="shop-method-area section-padding30">
    <div class="container">
      <div class="col-12">
        <div class="section-tittle mb-30 text-center">
          <h2>Meet our brands</h2>
        </div>
      </div>
      <div class="branddisplaysection">
        <% brandname.forEach(item=> {%> 
        <div id="imgbranddiv">
          <a href="/brand/<%= item.label%>"><img src="/brands/<%= item.value%>.png" alt="<%= item.value %> png" class="brandimg"></a>
        </div>
        <% }) %>
      </div>
    </div>
  </div>
</main>
<!-- Head END -->
<script src="/costom/javascript/cart.js"></script>

<script>
  window.addEventListener("DOMContentLoaded" , function () {
    $('.carousel').carousel({
  interval: 4000
})
  })
  
</script>